import java.util.*;
public class Main
{
  public static void main (String[]args)
  {
    Scanner s = new Scanner (System.in);
    int a = s.nextInt ();
    int arr[] = new int[a];
    int min=0;
    for (int i = 0; i < arr.length; i++)
      {
	arr[i] = s.nextInt ();
      }
      //length 
      System.out.println("length: "+arr.length);
for (int i = 0; i < arr.length; i++)
      {
    
        min+=arr[i];
      }
//total
      System.out.println("total"+min);

  }
}
